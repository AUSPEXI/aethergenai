# OpenSSL 3.x configuration with OQS provider
# Usage (example):
#   export OPENSSL_CONF=/path/to/config/openssl-oqs.cnf
#   export OQS_PROVIDER=1
#   openssl version -a

openssl_conf = openssl_init

[openssl_init]
providers = provider_sect
alg_section = algorithm_sect

[provider_sect]
default = default_sect
# Load OQS provider if available
oqsprovider = oqs_sect

[default_sect]
activate = 1

[oqs_sect]
activate = 1
module = oqsprovider

[algorithm_sect]
# Example: prefer hybrid key exchange groups where supported
# TLS groups will be set by application/server config, but this hints allowed properties
